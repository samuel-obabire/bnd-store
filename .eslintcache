scribeFromListener = productRef.onSnapshot(\r\n      doc => {\r\n        setSelectedProduct(doc.data())\r\n      },\r\n      e => console.log(e)\r\n    )\r\n\r\n    return () => unsubscribeFromListener()\r\n  }, [product])\r\n\r\n  const renderSimilarProducts = () => {\r\n    return similarProd.length ? (\r\n      <div style={{ marginTop: '2rem' }}>\r\n        <Collection\r\n          products={similarProd}\r\n          title=\"Similar Products\"\r\n          url={similarProd[0].category}\r\n        />\r\n      </div>\r\n    ) : null\r\n  }\r\n\r\n  const render = () => {\r\n    if (loading) return <Spinner />\r\n\r\n    if (!prod)\r\n      return (\r\n        <div style={{ marginBottom: '3rem' }}>\r\n          Product(s) might not exist or you don't have an internet connection\r\n        </div>\r\n      )\r\n\r\n    return (\r\n      <>\r\n        <ProductGallery />\r\n        {renderSimilarProducts()}\r\n      </>\r\n    )\r\n  }\r\n\r\n  return <main className=\"product-page\">{render()}</main>\r\n}\r\n\r\nconst mapState = state => {\r\n  return {\r\n    product: getUserSelectedProduct(state)\r\n  }\r\n}\r\n\r\nexport default connect(mapState, { setSelectedProduct })(ProductPage)\r\n","C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\Paystack.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\redux\\actions\\types.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\redux\\reducers\\noticationReducer.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\redux\\selectors\\index.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\product\\Product.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\product-gallery\\ProductGallery.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\hoc\\withMediaQuery.js",["281"],"import { useEffect, useState } from 'react'\r\n\r\nconst withMediaQuery = (Component, media) => ({ ...props }) => {\r\n  const [isMobile, setIsMobile] = useState()\r\n\r\n  const mql = window.matchMedia(media)\r\n\r\n  useEffect(() => {\r\n    const isMobile = e => {\r\n      setIsMobile(e.matches)\r\n    }\r\n\r\n    mql.addEventListener('change', isMobile)\r\n\r\n    return () => mql.removeEventListener('change', isMobile)\r\n  }, [])\r\n\r\n  return <Component {...props} isMobile={isMobile ?? mql.matches} />\r\n}\r\n\r\nexport default withMediaQuery\r\n","C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\404.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\pages\\shop-page\\ShopPage.js",["282"],"import { Redirect, Route, Switch } from 'react-router-dom'\r\nimport { lazy, Suspense, useEffect } from 'react'\r\nimport { connect } from 'react-redux'\r\n\r\nimport './ShopPage.scss'\r\n\r\nimport { setQuery } from '../../../redux/actions'\r\n\r\nimport useQuery from '../../../hooks/useQuery'\r\n// import Error from '../../404'\r\nimport Spinner from '../../spinner/Spinner'\r\n\r\nconst AllCollections = lazy(() =>\r\n  import('../../all-collections/AllCollections')\r\n)\r\nconst ShopCollection = lazy(() =>\r\n  import('../../shop-collection/ShopCollection')\r\n)\r\nconst ProductPage = lazy(() => import('../../product-page/ProductPage'))\r\n\r\nconst ShopPage = ({ setQuery, match, location }) => {\r\n  const query = useQuery()\r\n\r\n  useEffect(() => {\r\n    if (!query.toString()) return\r\n\r\n    const [field, q] = [query.get('field'), query.get('q')]\r\n    setQuery({ field, q })\r\n  }, [setQuery, location.search])\r\n\r\n  return (\r\n    <main className=\"shop-container container\">\r\n      <Suspense\r\n        fallback={\r\n          <div className=\"fallback\">\r\n            <Spinner />\r\n          </div>\r\n        }>\r\n        <Switch>\r\n          <Route exact path=\"/shop\" component={AllCollections} />\r\n          <Route\r\n            exact\r\n            path=\"/shop/collection/:title\"\r\n            component={ShopCollection}\r\n          />\r\n          <Route\r\n            exact\r\n         