  setItems(items)\r\n  }\r\n\r\n  const nextPage = async () => {\r\n    if (Math.floor(totalProd / (pageSize * pageNum)) <= 0) return\r\n\r\n    setPageNum(pageNum + 1)\r\n\r\n    clearProducts()\r\n\r\n    window.scrollTo(0, 0)\r\n\r\n    const q = pageRef.orderBy(filter).startAfter(lastVisible).limit(pageSize)\r\n    setItems(await transformItems(q))\r\n  }\r\n\r\n  const previousPage = async () => {\r\n    console.log(pageNum)\r\n    if (pageNum === 1) return\r\n\r\n    clearProducts()\r\n\r\n    window.scrollTo(0, 0)\r\n\r\n    const q = pageRef\r\n      .orderBy(filter)\r\n      .endBefore(firstVisible)\r\n      .limitToLast(pageSize)\r\n\r\n    setItems(await transformItems(q))\r\n    setPageNum(pageNum - 1)\r\n  }\r\n\r\n  useEffect(() => {\r\n    setProducts(items)\r\n  }, [items])\r\n\r\n  useEffect(() => {\r\n    clearProducts()\r\n    defaultPage()\r\n  }, [query])\r\n\r\n  const shopProducts = Object.values(products).map(({ ...props }) => {\r\n    return (\r\n      <div className=\"shop-collection\" key={props.id}>\r\n        <Product {...props} />\r\n      </div>\r\n    )\r\n  })\r\n\r\n  const renderShopProducts = () => {\r\n    const hasMoreFwd =\r\n      Math.floor(totalProd / (pageSize * pageNum)) <= 0 ? 'disabled' : ''\r\n    const hasMoreBkw = pageNum === 1 ? 'disabled' : ''\r\n    const currentPage = `${pageNum}/${Math.ceil(totalProd / pageSize)}`\r\n\r\n    return (\r\n      <main className=\"shop-wrapper container\">\r\n        <h1>{pageTitle}</h1>\r\n        <div className=\"shop-collection-container\">{shopProducts}</div>\r\n        <div className=\"page-nav-btn\">\r\n          <button className={`btn-link ${hasMoreBkw}`} onClick={previousPage}>\r\n            PREVIOUS PAGE\r\n          </button>\r\n          <div>{currentPage}</div>\r\n          <button className={`btn-link ${hasMoreFwd}`} onClick={nextPage}>\r\n            NEXT PAGE\r\n          </button>\r\n        </div>\r\n      </main>\r\n    )\r\n  }\r\n\r\n  return shopProducts.length ? renderShopProducts() : <Spinner />\r\n}\r\n\r\nconst mapState = state => {\r\n  return {\r\n    products: state.shop.products,\r\n    query: state.shop.query\r\n  }\r\n}\r\n\r\nexport default connect(mapState, { setProducts, clearProducts })(ShopCollection)\r\n",["263","264"],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\product-page\\ProductPage.js",["265","266"],"import { connect } from 'react-redux'\r\nimport { useEffect, useState } from 'react'\r\n\r\nimport './ProductPage.scss'\r\n\r\nimport useQuery from '../../hooks/useQuery'\r\nimport ProductGallery from '../product-gallery/ProductGallery'\r\nimport { firestore } from '../utils/firebase'\r\nimport { setSelectedProduct } from '../../redux/actions'\r\nimport Collection from '../collection/Collection'\r\nimport { useLocation } from 'react-router-dom'\r\nimport Spinner from '../spinner/Spinner'\r\n\r\nconst ProductPage = ({ setSelectedProduct }) => {\r\n  const id = useQuery().get('id')\r\n  const [product, setProduct] = useState(null)\r\n  const [similarProd, setSimilarProd] = useState([])\r\n  const [loading, setLoading] = useState(true)\r\n\r\n  const search = useLocation().search\r\n\r\n  useEffect(() => {\r\n    setLoading(true)\r\n  }, [search])\r\n\r\n  useEffect(() => {\r\n    if (!id) return\r\n    const productRef = firestore.collection('products').doc(id)\r\n\r\n    const unsubscribeFromListener = productRef.onSnapshot(\r\n      doc => {\r\n        setProduct(doc.data())\r\n\r\n        setSelectedProduct(doc.data())\r\n        setLoading(false)\r\n      },\r\n      e => console.log(e)\r\n    )\r\n\r\n    return () => unsubscribeFromListener()\r\n  }, [search])\r\n\r\n  useEffect(() => {\r\n    if (!product) return\r\n\r\n    const productRef = firestore.collection('products').doc(id)\r\n\r\n    firestore\r\n      .collection('products')\r\n      .where('category', '==', product.category)\r\n      .get()\r\n      .then(querySnapshot => {\r\n        return querySnapshot.docs\r\n          .map(doc => doc.data())\r\n          .sort(() => Math.random() - 0.5)\r\n          .filter((prod, idx) => prod.id !== product.id && idx < 6)\r\n      })\r\n      .then(setSimilarProd)\r\n      .catch(e => console.log(e))\r\n\r\n    const unsubscribeFromListener = productRef.onSnapshot(\r\n      doc => {\r\n        setSelectedProduct(doc.data())\r\n      },\r\n      e => console.log(e)\r\n    )\r\n\r\n    return () => unsubscribeFromListener()\r\n  }, [product])\r\n\r\n  const renderSimilarProducts = () => {\r\n    return similarProd.length ? (\r\n      <div style={{ marginTop: '2rem' }}>\r\n        <Collection\r\n          products={similarProd}\r\n          title=\"Similar Products\"\r\n          url={similarProd[0].category}\r\n        />\r\n      </div>\r\n    ) : null\r\n  }\r\n\r\n  const render = () => {\r\n    if (loading) return <Spinner />\r\n\r\n    return (\r\n      <>\r\n        <ProductGallery />\r\n        {renderSimilarProducts()}\r\n      </>\r\n    )\r\n  }\r\n\r\n  return <main className=\"product-page\">{render()}</main>\r\n}\r\n\r\nexport default connect(null, { setSelectedProduct })(ProductPage)\r\n","C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\Paystack.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\redux\\actions\\types.js",[],["267","268"],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\redux\\reducers\\noticationReducer.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\redux\\selectors\\index.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\product\\Product.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\product-gallery\\ProductGallery.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\hoc\\withMediaQuery.js",["269"],"import { useEffect, useState } from 'react'\r\n\r\nconst withMediaQuery = (Component, media) => ({ ...props }) => {\r\n  const [isMobile, setIsMobile] = useState()\r\n\r\n  const mql = window.matchMedia(media)\r\n\r\n  useEffect(() => {\r\n    const isMobile = e => {\r\n      setIsMobile(e.matches)\r\n    }\r\n\r\n    mql.addEventListener('change', isMobile)\r\n\r\n    return () => mql.removeEventListener('change', isMobile)\r\n  }, [])\r\n\r\n  return <Component {...props} isMobile={isMobile ?? mql.matches} />\r\n}\r\n\r\nexport default withMediaQuery\r\n","C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\404.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\pages\\shop-page\\ShopPage.js",["270"],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\search-bar\\SearchBar.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\lazy-image\\LazyImage.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\form\\Form.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\items-collection\\ItemsCollection.js",[],["271","272"],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\redux\\reducers\\shopReducer.js",["273"],"import {\r\n  SET_CATEGORIES,\r\n  SET_PRODUCTS,\r\n  SET_QUERY,\r\n  CLEAR_PRODUCTS\r\n} from '../actions/types'\r\nconst INITIAL_STATE = {\r\n  categories: [\r\n    // 'Women Clothing',\r\n    // 'Electronics',\r\n    // 'Slippers',\r\n    // 'Fabrics',\r\n    // 'Hair Accessories',\r\n    // 'Bags'\r\n  ],\r\n  categories: {},\r\n  products: {},\r\n  query: {\r\n    q: '',\r\n    field: ''\r\n  }\r\n}\r\n\r\nconst shopReducer = (state = INITIAL_STATE, action) => {\r\n  switch (action.type) {\r\n    case SET_CATEGORIES:\r\n      const category = action.payload.category\r\n      return {\r\n        ...state,\r\n        categories: {\r\n          ...state.categories,\r\n          [category]: action.payload[category]\r\n        }\r\n      }\r\n    case CLEAR_PRODUCTS:\r\n      return { ...state, products: {} }\r\n    case SET_PRODUCTS:\r\n      return { ...state, products: action.payload }\r\n    case SET_QUERY:\r\n      return { ...state, query: action.payload }\r\n    default:\r\n      return state\r\n  }\r\n}\r\n\r\nexport default shopReducer\r\n","C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\redux\\reducers\\cartReducer.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\hooks\\useMediaQuery.js",["274"],"import { useEffect, useState } from 'react'\r\n\r\nconst useMediaQuery = mediaQuery => {\r\n  const [isMedia, setIsmedia] = useState()\r\n  const [vw, setVw] = useState(document.documentElement.clientWidth)\r\n  const [vh, setVh] = useState(document.documentElement.clientHeight)\r\n\r\n  const mql = window.matchMedia(mediaQuery)\r\n\r\n  useEffect(() => {\r\n    const reportWindowSize = e => {\r\n      setVh(document.documentElement.clientHeight)\r\n      setVw(document.documentElement.clientWidth)\r\n    }\r\n\r\n    const isMedia = e => {\r\n      setIsmedia(e.matches)\r\n    }\r\n\r\n    window.addEventListener('resize', reportWindowSize)\r\n\r\n    if (!mediaQuery)\r\n      return () => window.removeEventListener('resize', reportWindowSize)\r\n\r\n    mql.addEventListener('change', isMedia)\r\n\r\n    return () => {\r\n      mql.removeEventListener('change', isMedia)\r\n      window.removeEventListener('resize', reportWindowSize)\r\n    }\r\n  }, [])\r\n\r\n  return { isMedia: mediaQuery ? isMedia ?? mql.matches : null, vw, vh }\r\n}\r\n\r\nexport default useMediaQuery\r\n","C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\ScrollToTop.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\pages\\sign-up-page\\SignupPage.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\all-collections\\AllCollections.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\collection-overview\\CollectionOverview.js",[],["275","276"],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\redux\\reducers\\setMobileMenuVisibilityReducer.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\redux\\reducers\\userReducer.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\collection\\Collection.js",[],"C