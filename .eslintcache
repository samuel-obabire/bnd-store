{\r\n        return querySnapshot.docs\r\n          .map(doc => doc.data())\r\n          .sort(() => Math.random() - 0.5)\r\n          .filter((prod, idx) => prod.id !== product.id && idx < 6)\r\n      })\r\n      .then(setSimilarProd)\r\n      .catch(e => console.log(e))\r\n\r\n    const unsubscribeFromListener = productRef.onSnapshot(\r\n      doc => {\r\n        setSelectedProduct(doc.data())\r\n      },\r\n      e => console.log(e)\r\n    )\r\n\r\n    return () => unsubscribeFromListener()\r\n  }, [product])\r\n\r\n  const renderSimilarProducts = () => {\r\n    return similarProd.length ? (\r\n      <div style={{ marginTop: '2rem' }}>\r\n        <Collection\r\n          products={similarProd}\r\n          title=\"Similar Products\"\r\n          url=\"electronics\"\r\n        />\r\n      </div>\r\n    ) : null\r\n  }\r\n\r\n  const render = () => {\r\n    if (loading) return <Spinner />\r\n\r\n    return (\r\n      <>\r\n        <ProductGallery />\r\n        {renderSimilarProducts()}\r\n      </>\r\n    )\r\n  }\r\n\r\n  return <main className=\"product-page\">{render()}</main>\r\n}\r\n\r\nexport default connect(null, { setSelectedProduct })(ProductPage)\r\n","C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\custom-btn\\CustomBtn.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\redux\\reducers\\userReducer.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\utils\\cartUtils.js",[],["250","251"],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\pages\\cart-page\\CartPage.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\redux\\reducers\\cartReducer.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\mobile-nav\\MobileNav.js",[],["252","253"],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\404.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\pages\\checkout-page\\CheckoutPage.js",["254"],"import './CheckoutPage.scss'\r\n\r\nimport NaijaStates from 'naija-state