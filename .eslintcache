 {} }\r\n    case SET_PRODUCTS:\r\n      return { ...state, products: action.payload }\r\n    case SET_QUERY:\r\n      return { ...state, query: action.payload }\r\n    default:\r\n      return state\r\n  }\r\n}\r\n\r\nexport default shopReducer\r\n","C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\redux\\reducers\\cartReducer.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\hooks\\useMediaQuery.js",["275"],"import { useEffect, useState } from 'react'\r\n\r\nconst useMediaQuery = mediaQuery => {\r\n  const [isMedia, setIsmedia] = useState()\r\n  const [vw, setVw] = useState(document.documentElement.clientWidth)\r\n  const [vh, setVh] = useState(document.documentElement.clientHeight)\r\n\r\n  const mql = window.matchMedia(mediaQuery)\r\n\r\n  useEffect(() => {\r\n    const reportWindowSize = e => {\r\n      setVh(document.documentElement.clientHeight)\r\n      setVw(document.documentElement.clientWidth)\r\n    }\r\n\r\n    const isMedia = e => {\r\n      setIsmedia(e.matches)\r\n    }\r\n\r\n    window.addEventListener('resize', reportWindowSize)\r\n\r\n    if (!mediaQuery)\r\n      return () => window.removeEventListener('resize', reportWindowSize)\r\n\r\n    mql.addEventListener('change', isMedia)\r\n\r\n    return () => {\r\n      mql.removeEventListener('change', isMedia)\r\n      window.removeEventListener('resize', reportWindowSize)\r\n    }\r\n  }, [])\r\n\r\n  return { isMedia: mediaQuery ? isMedia ?? mql.matches : null, vw, vh }\r\n}\r\n\r\nexport default useMediaQuery\r\n","C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\ScrollToTop.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\pages\\sign-up-page\\SignupPage.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\all-collections\\AllCollections.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\components\\collection-overview\\CollectionOverview.js",[],"C:\\Users\\sam__olex\\workspace\\bnd-store\\src\\redux\\reducers\\setMobileMenuVisibilityRe